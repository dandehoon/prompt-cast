{
  "name": "prompt-cast",
  "description": "ğŸ“¢ Broadcast your prompts to multiple AI sites at once",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:test": "wxt build -c wxt.config.test.ts",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "test": "pnpm test:unit && pnpm e2e",
    "test:unit": "vitest run",
    "e2e": "pnpm build:test && playwright test --reporter=line",
    "e2e:ui": "pnpm build:test && playwright test --ui",
    "test:server": "tsx tests/e2e/server.ts",
    "test:coverage": "vitest run --coverage",
    "check": "pnpm type-check && pnpm lint && pnpm test && pnpm build",
    "lint": "eslint src --ext .ts,.svelte --fix",
    "type-check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "keywords": [
    "chrome-extension",
    "ai",
    "chatgpt",
    "claude",
    "gemini",
    "grok",
    "perplexity",
    "copilot",
    "wxt",
    "svelte"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/svelte": "^5.2.8",
    "@tsconfig/svelte": "^5.0.4",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@webext-core/messaging": "^2.3.0",
    "@wxt-dev/module-svelte": "^2.0.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-svelte": "^3.11.0",
    "express": "^5.1.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "svelte": "^5.38.0",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4",
    "wxt": "^0.20.7"
  },
  "browserslist": [
    "chrome >= 100"
  ],
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
